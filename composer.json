        }
    },
    "require": {
        "php": "^5.4|^7.0",
        "ext-mbstring": "*",
        "beberlei/assert": "^2.4",
        "lib-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5|^5.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
